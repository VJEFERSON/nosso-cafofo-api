{
  "name": "nosso-cafofo-api",
  "version": "1.0.0",
  "description": "Api do Nosso Cafofo para suprir a Aplicação Web e Aplicativos Mobile.",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "knex:migrate:dev": "knex --knexfile knexfile.ts migrate:latest",
    "knex:migrate:rollback:dev": "knex --knexfile knexfile.ts migrate:rollback",
    "knex:migrate": "knex --knexfile dist/knexfile.js migrate:latest",
    "knex:migrate:rollback": "knex --knexfile dist/knexfile.js migrate:rollback",
    "start": "node dist/src/server.js",
    "start:dev": "tsnd --transpile-only --ignore-watch node_modules --respawn src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VJEFERSON/nosso-cafofo-api.git"
  },
  "keywords": [],
  "author": "Valdeci Jeferson Pereira Rocha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VJEFERSON/nosso-cafofo-api/issues"
  },
  "homepage": "https://github.com/VJEFERSON/nosso-cafofo-api#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "moment": "^2.29.1",
    "pg": "^8.7.1",
    "swagger-ui-express": "^4.1.6",
    "uninstall": "0.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.7.10",
    "@types/swagger-ui-express": "^4.1.3",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": "14.x",
    "npm": "6.x",
    "yarn": "1.x"
  }
}